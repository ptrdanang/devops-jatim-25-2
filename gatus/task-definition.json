{
  
  "family": "Iks-gatus-app",
  "taskRoleArn": "arn:aws:iam::211125520056:role/LabRole",
  "executionRoleArn": "arn:aws:iam::211125520056:role/LabRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities":  [
    "FARGATE"],

  "cpu": "1024",
  "memory": "3072",
 

  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "enableFaultInjection": false,
  "containerDefinitions": [
    {
      "name": "gatus",
      "image": "<task-def-image>", 
      "cpu": 0,
      "essential": true,
      "volumesFrom": [],
      "systemControls": [],

      
      "portMappings": [
        {
          "appProtocol": "http",
          "containerPort": 8080,
          "hostPort": 8080,
          "name": "gatus-8080-tcp",
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/Iks-gatus-app",
          "awslogs-create-group": "true",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
      
    }
  ]
}