{
  "family": "lks-leantime-app",
  "taskRoleArn": "arn:aws:iam::748381872886:role/LabRole",
  "executionRoleArn": "arn:aws:iam::748381872886:role/LabRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "2048",
  "volumes": [
    {
      "name": "public-files",
      "efsVolumeConfiguration": {
        "fileSystemId": "fs-0b9290b5a251eeb61",
        "rootDirectory": "/",
        "transitEncryption": "ENABLED",
        "authorizationConfig": {
          "accessPointId": "fsap-07e2f3b222945e82e",
          "iam": "ENABLED"
        }
      }
    },
    {
      "name": "userfiles",
      "efsVolumeConfiguration": {
        "fileSystemId": "fs-0b9290b5a251eeb61",
        "rootDirectory": "/",
        "transitEncryption": "ENABLED",
        "authorizationConfig": {
          "accessPointId": "fsap-093468a685581477b",
          "iam": "ENABLED"
        }
      }
    },
    {
      "name": "plugins",
      "efsVolumeConfiguration": {
        "fileSystemId": "fs-0b9290b5a251eeb61",
        "rootDirectory": "/",
        "transitEncryption": "ENABLED",
        "authorizationConfig": {
          "accessPointId": "fsap-088149353e4b6daab",
          "iam": "ENABLED"
        }
      }
    }
  ],
  "placementConstraints": [],
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "containerDefinitions": [
    {
      "name": "leantime",
      "image": "leantime/leantime:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "secrets": [
        {
          "name": "LEAN_DB_DATABASE",
          "valueFrom": "arn:aws:ssm:us-east-1:748381872886:parameter/lks/db/name"
        },
        {
          "name": "LEAN_DB_HOST ",
          "valueFrom": "arn:aws:ssm:us-east-1:748381872886:parameter/lks/db/host"
        },
        {
          "name": "LEAN_DB_PASSWORD ",
          "valueFrom": "arn:aws:ssm:us-east-1:748381872886:parameter/lks/db/password"
        },
        {
          "name": "LEAN_DB_PORT",
          "valueFrom": "arn:aws:ssm:us-east-1:748381872886:parameter/lks/db/port"
        },
        {
          "name": "LEAN_DB_USER",
          "valueFrom": "arn:aws:ssm:us-east-1:748381872886:parameter/lks/db/username"
        }
      ],
      "environment": [
        {
          "name": "PUID",
          "value": "1000"
        },
        {
          "name": "PGID",
          "value": "1000"
        },
        {
          "name": "LEAN_PORT",
          "value": "8080"
        },
        {
          "name": "LEAN_DEBUG",
          "value": "0"
        },
        {
          "name": "LEAN_SITENAME",
          "value": "LKS Leantime"
        },
        {
          "name": "LEAN_LANGUAGE",
          "value": "en-US"
        },
        {
          "name": "LEAN_DEFAULT_TIMEZONE",
          "value": "Asia/Jakarta"
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "public-files",
          "containerPath": "/var/www/html/public/userfiles",
          "readOnly": false
        },
        {
          "sourceVolume": "userfiles",
          "containerPath": "/var/www/html/userfiles",
          "readOnly": false
        },
        {
          "sourceVolume": "plugins",
          "containerPath": "/var/www/html/app/Plugins",
          "readOnly": false
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/lks/leantime-app",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      }
    }
  ]
}